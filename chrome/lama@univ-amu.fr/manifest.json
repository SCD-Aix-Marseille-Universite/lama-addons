{
    "name": "lama",
    "description": "Accès hors campus aux ressources électroniques documentaires de l'Université d'Aix-Marseille",
    "version": "0.2",
    "manifest_version": 2,
    "permissions": [ "http://*/*", "https://*/*", "tabs" ],
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
        },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "button.png",
        "default_title": "Accès hors campus"
    }
}