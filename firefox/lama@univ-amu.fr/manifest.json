{

  "description": "Accès hors campus aux ressources électroniques documentaires de l'Université d'Aix-Marseille",
  "manifest_version": 2,
  "name": "lama",
  "version": "0.3",
  "icons": {
    "48": "icons/lama-48.png"
  },
  
  "permissions": [
    "activeTab"
  ],

  "applications": {
    "gecko": {
      "id": "lama@univ-amu.fr",
      "strict_min_version": "42.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/lama-32.png"
  }

}
